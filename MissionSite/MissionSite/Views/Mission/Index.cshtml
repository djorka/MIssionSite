@model IEnumerable<MissionSite.Models.Mission>

@{
    ViewBag.Title = "Index";
}

<div class="jumbotron mission-header text-center">
    <div class="dropdown">
        <button class="btn btn-primary btn-lg dropdown-toggle" type="button" data-toggle="dropdown">
            Select a Mission
            <span class="caret"></span>
        </button>
        <ul class="dropdown-menu">
            @foreach (var item in Model)
            {
                <li>@Html.ActionLink(item.MisName, "MissionPage", "Mission", new { MisName = item.MisName }, new { @class = "btn" })</li>
            }
        </ul>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <iframe width="560" height="315" src="https://www.youtube.com/embed/W0-0nniCz6k" frameborder="0" allowfullscreen></iframe>
    </div>
    <div class="col-md-6">
        <iframe width="560" height="315" src="https://www.youtube.com/embed/PpEhE_scrs4" frameborder="0" allowfullscreen></iframe>
    </div>
</div>







